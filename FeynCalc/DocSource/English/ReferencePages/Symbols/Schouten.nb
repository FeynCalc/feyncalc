Cell[TextData[{
 ButtonBox["Schouten",
  BaseStyle->"Link",
  ButtonData:>"paclet:FeynCalc/ref/Schouten",
  ButtonNote->"Schouten"],
 " applies the identity for 4-vectors on at most ",
 Cell[BoxData["42"], "InlineFormula",
  FormatType->"StandardForm"],
 " terms in a sum."
}], "Notes"],

Cell[TextData[{
