	FAD[{l, MC}, k1 + l, {-k2 + l + p2, MC}]*(MC^2 - SPD[k1, k1] - 2*SPD[k1, k2] +
	2*SPD[k1, p2] - SPD[k2, k2] + 2*SPD[k2, p2] - SPD[p2, p2]))/2"}
